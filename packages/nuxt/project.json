{
  "name": "nuxt",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/nuxt/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/nuxt",
        "commands": [
          "npm run lint",
          "npm run prepack",
          "rm -fr ../../dist/nuxt",
          "mkdir -p ../../dist/nuxt",
          "cp -r ./dist/* ../../dist/nuxt/",
          "cp -r ./package.json ../../dist/nuxt/package.json"
        ],
        "parallel": false
      }
    }
  }
}
